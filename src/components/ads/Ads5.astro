---
// Ads5.astro
const { slotId } = Astro.props;
---

<ins class="adsbygoogle"
     style="display:block"
     data-ad-client="ca-pub-XXXX"
     data-ad-slot={slotId}
     data-ad-format="auto"
     data-full-width-responsive="true">
</ins>

<script is:inline>
  (adsbygoogle = window.adsbygoogle || []).push({});
</script>
